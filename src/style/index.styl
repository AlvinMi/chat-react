body, h1, h2, h3, h4, h5, h6, dl, dt, dd, ul, ol, li, th, td, p, blockquote, pre, form, fieldset, legend, input, button, textarea, hr {
  margin: 0;
  padding: 0
}

h1, h2, h3, h4, h5, h6 {
  font-size: 100%;
  font-weight: normal;
}

* {
  box-sizing: border-box;
  font-variant-ligatures: none;
  letter-spacing: 0px;
}

li {
  list-style: none
}

fieldset, img {
  border: 0
}

