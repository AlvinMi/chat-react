@-webkit-keyframes fadeInUp{
  0%{ opacity:0; -webkit-transform:translate3d(0, 20%, 0); transform:translate3d(0, 20%, 0) }
  100%{ opacity:1; -webkit-transform:none; transform:none }
}

@keyframes fadeInUp{
  0%{ opacity:0; -webkit-transform:translate3d(0, 20%, 0); -ms-transform:translate3d(0, 20%, 0); transform:translate3d(0, 20%, 0) }
  100%{ opacity:1; -webkit-transform:none; -ms-transform:none; transform:none }
}

.fadeInUp{ -webkit-animation-name:fadeInUp; animation-name:fadeInUp;-webkit-animation-duration:0.8s; animation-duration:0.8s; -webkit-animation-fill-mode:both; animation-fill-mode:both;display:block !important }


